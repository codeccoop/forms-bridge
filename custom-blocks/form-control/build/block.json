{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpct/form-control",
  "version": "0.1.0",
  "title": "Wpct Form Control",
  "category": "wpct",
  "icon": "input",
  "description": "Form control gutenberg custom block",
  "ancestor": [
    "wpct/form"
  ],
  "attributes": {
    "name": {
      "type": "text",
      "default": "field name"
    },
    "type": {
      "type": "text",
      "default": ""
    },
    "placeholder": {
      "type": "text",
      "default": ""
    },
    "defaultValue": {
      "type": "text",
      "default": null
    }
  },
  "supports": {
    "html": false
  },
  "example": {
    "attributes": {
      "type": "text",
      "label": "Foo",
      "placeholder": "Enter some text..."
    }
  },
  "textdomain": "wpct-form",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "keywords": [
    "wpct",
    "forms",
    "control",
    "backend"
  ]
}